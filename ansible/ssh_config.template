
Host *
  ForwardAgent yes

Host {collator}
  User ubuntu
  ForwardAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ControlPath ~/.ssh/cm-%r@%h:%p
  ControlMaster auto
  ControlPersist 10m

Host 10.0.*
  User ubuntu
  ProxyJump {collator}
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
